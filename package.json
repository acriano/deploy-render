{
  "name": "rest-express",
  "version": "1.0.0",
  "type": "module",
  "license": "MIT",
  "scripts": {
    "dev": "concurrently \"npm run dev:server\" \"npm run dev:client\"",
    "dev:server": "cross-env NODE_ENV=development dotenv -e .env.development -- tsx server/index.ts",
    "dev:client": "dotenv -e .env.development -- vite",
    "build:server": "tsc -p tsconfig.server.json && xcopy scripts dist\\scripts /E /I /Y",
    "build:server-only": "tsc --project tsconfig.server.json --skipLibCheck && xcopy scripts dist\\scripts /E /I /Y",
    "build:backend": "tsc --project tsconfig.backend.json && cp -r scripts dist/scripts",
    "build:backend-safe": "move client client_temp && tsc --project tsconfig.backend.json && xcopy scripts dist\\scripts /E /I /Y && move client_temp client",
    "build:backend-final": "powershell -Command \"(Get-Content server/vite.ts) -replace 'const viteConfig = \\(await import\\(\"\\\\.\\./client/vite\\.config\\.mjs\"\\)\\)\\.default;', '// const viteConfig = (await import(\"../client/vite.config.mjs\")).default;' -replace '\\.\\.\\.viteConfig,', '// ...viteConfig,' | Set-Content server/vite.ts\" && move client client_temp && tsc --project tsconfig.backend.json && xcopy scripts dist\\scripts /E /I /Y && move client_temp client && powershell -Command \"(Get-Content server/vite.ts) -replace '// const viteConfig = \\(await import\\(\"\\\\.\\./client/vite\\.config\\.mjs\"\\)\\)\\.default;', 'const viteConfig = (await import(\"../client/vite.config.mjs\")).default;' -replace '// \\.\\.\\.viteConfig,', '...viteConfig,' | Set-Content server/vite.ts\"",
    "build:deploy": "cd client && npm run build && cd .. && npm run build:backend",
    "build": "npm run build:deploy",
    "start": "NODE_ENV=production node dist/server/index.js",
    "start:prod": "node scripts/init-production.cjs && npm start",
    "build:start": "npm run build && npm run start",
    "build:dev": "npm run build && npm run dev",
    "start:dashboard": "node start-dashboard-3000.js",
    "start:db": "node start-database.js",
    "check": "tsc",
    "db:push": "drizzle-kit push",
    "db:seed": "node seed.js",
    "db:seed-points": "node scripts/seed-collection-points.js",
    "db:add-points": "node seed-points.js",
    "db:backup": "node scripts/backup-db.js",
    "db:backup-postgres": "node scripts/backup-postgres.js",
    "db:check-admin": "node scripts/check-admin.js",
    "test:register": "node scripts/test-register.js",
    "clean:db": "node scripts/clean-db.js",
    "admin": "start start-admin.bat",
    "migrate": "ts-node server/migrate.ts",
    "migrate:init": "node server/init-db.js",
    "migrate:data": "ts-node server/migrate-data.ts",
    "migrate:postgres": "node migrate-to-postgres.js",
    "drop:tables": "node drop-tables.js",
    "check:schema": "node check-sqlite-schema.js",
    "verify:migration": "node verify-migration.js",
    "check:deploy": "node scripts/check-deploy.js",
    "start:render": "npm run build && npm run start:prod"
  },
  "dependencies": {
    "@hookform/resolvers": "^3.10.0",
    "@jridgewell/trace-mapping": "^0.3.25",
    "@libsql/client": "^0.15.4",
    "@neondatabase/serverless": "^0.10.4",
    "@radix-ui/react-accordion": "^1.2.4",
